{
  "name": "@loranza/spl-system",
  "version": "0.1.0",
  "description": "Program client from the System program",
  "sideEffects": false,
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup && tsc -p ./tsconfig.declarations.json",
    "build:docs": "typedoc",
    "test": "ava",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "format": "prettier --check src test",
    "format:fix": "prettier --write src test"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": "https://github.com/lorisleiva/solana-core-programs.git",
  "author": "Loris Leiva",
  "license": "MIT",
  "dependencies": {
    "@solana/accounts": "2.0.0-experimental.12ae1c6",
    "@solana/addresses": "2.0.0-experimental.12ae1c6",
    "@solana/codecs-core": "2.0.0-experimental.12ae1c6",
    "@solana/codecs-data-structures": "2.0.0-experimental.12ae1c6",
    "@solana/codecs-numbers": "2.0.0-experimental.12ae1c6",
    "@solana/codecs-strings": "2.0.0-experimental.12ae1c6",
    "@solana/instructions": "2.0.0-experimental.12ae1c6",
    "@solana/keys": "2.0.0-experimental.12ae1c6",
    "@solana/options": "2.0.0-experimental.12ae1c6",
    "@solana/programs": "2.0.0-experimental.12ae1c6",
    "@solana/signers": "2.0.0-experimental.12ae1c6",
    "@solana/transactions": "2.0.0-experimental.12ae1c6",
    "tsup": "^8.0.2"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@solana/web3.js": "2.0.0-experimental.12ae1c6",
    "@solana/webcrypto-ed25519-polyfill": "2.0.0-experimental.12ae1c6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.46.1",
    "ava": "^6.1.1",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.16",
    "typedoc-plugin-expand-object-like-types": "^0.1.1",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^5.3.3"
  },
  "ava": {
    "require": [
      "./test/_globals.js"
    ],
    "typescript": {
      "compile": "tsc",
      "rewritePaths": {
        "src/": "dist/ava/src/",
        "test/": "dist/ava/test/"
      }
    }
  },
  "packageManager": "pnpm@8.2.0"
}
